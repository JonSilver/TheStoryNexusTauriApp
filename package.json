{
    "name": "thestorynexus",
    "displayName": "The Story Nexus",
    "description": "A story writing app",
    "author": "Jon Silver",
    "private": true,
    "version": "0.3.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
        "dev:server": "cross-env NODE_ENV=development PORT=3001 tsx watch server/index.ts",
        "dev:client": "vite",
        "build": "tsc && tsc --project server/tsconfig.json && vite build",
        "start": "cross-env NODE_ENV=production node dist/server/index.js",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "knip": "knip"
    },
